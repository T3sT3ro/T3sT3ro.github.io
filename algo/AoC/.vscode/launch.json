{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node.js - launch current file",
            "program": "${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**",
                "**/lodash/**"
            ],
            "type": "node",
            "cwd": "${fileDirname}",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TS launch",
            "program": "${file}",
            "preLaunchTask": "tsc: build - util/tsconfig.json",
            "cwd": "${fileDirname}",
        }
    ]
}
